---
interface Props {
  src: string;
  imgClasses?: string;
  figureClasses?: string;
}
import { cn } from "@/lib/utils";
const { src, imgClasses, figureClasses } = Astro.props;
const finalFigClasses = cn(figureClasses);
const finalImgClasses = cn(imgClasses);
---

<figure class:list={["", finalFigClasses]}>
  <a href="/" aria-label="Home">
    <img class:list={["", finalImgClasses]} src={src} alt={"Colorful logo of the company"} />
  </a>
</figure>
